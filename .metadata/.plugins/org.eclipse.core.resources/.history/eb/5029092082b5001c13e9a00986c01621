import java.util.Scanner;

public class sampleuserinput2 {

	public static void main(String[] args) {
		System.out.println("Enter the Size");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		for(int i=0;i<n;i++) {
			for(int j=0;j<n;j++) {
				if(j==0 && i<=(n-1)) {
					System.out.print("*");
				}
			}
			System.out.println();
		}

	}

}
